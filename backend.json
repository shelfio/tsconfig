{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "target": "esnext",
    "strict": true,
    "noErrorTruncation": true
  }
}
